<web-app>
	<welcome-file-list>
		<welcome-file>login.html</welcome-file>
		<welcome-file>signUp.html</welcome-file>
	</welcome-file-list>

<servlet>
	<servlet-name>login</servlet-name>
		<servlet-class>com.servlet.Login</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>sign up</servlet-name>
		<servlet-class>com.servlet.SignUp</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>sign up</servlet-name>
		<url-pattern>/signUp</url-pattern>
</servlet-mapping>

</web-app>